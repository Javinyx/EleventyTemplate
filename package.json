{
    "name": "javinyx-blog",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "build": "npm-run-all build:css build:html",
        "build:css": "npx sass src/sass:public/css",
        "build:html": "npx @11ty/eleventy",
        "watch": "npm-run-all --parallel watch:css watch:html",
        "watch:css": "npx sass --watch src/sass:public/css",
        "watch:html": "npx @11ty/eleventy --serve --port=8181",
        "start": "npm run watch"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Javinyx/javinyx-blog.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Javinyx/javinyx-blog/issues"
    },
    "homepage": "https://github.com/Javinyx/javinyx-blog#readme",
    "dependencies": {
        "@11ty/eleventy": "^0.12.1"
    },
    "devDependencies": {
        "npm-run-all": "^4.1.5",
        "sass": "^1.35.2"
    }
}
